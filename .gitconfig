[color]
    diff = auto
    status = auto
    branch = auto
    interactive = auto
    ui = auto

[merge]
    #tool = meld
    tool = diffuse
    #tool = diffuse

[diff]
   tool = diffuse
   suppressBlankEmpty = true
   mnemonicprefix = true
   #noprefix = true

[push]
   default = tracking

[branch]
   autosetupmerge = true

[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green

[color "diff"]
  meta = yellow bold
  frag = magenta
  plain = white bold
  old = red bold
  new = green bold
  commit = yellow bold
  func = green dim
  #plain = white bold

[color "status"]
  added = yellow
  changed = green
  untracked = cyan

